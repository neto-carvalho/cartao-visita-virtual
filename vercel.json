{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "backend/**"
    },
    "api/health.js": {}
  },
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    },
    {
      "source": "/public/:path*",
      "destination": "/api/index"
    }
  ]
}

