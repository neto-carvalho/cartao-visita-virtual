{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "backend/**"
    }
  },
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/public/:path*",
      "destination": "/api/index.js"
    }
  ]
}

