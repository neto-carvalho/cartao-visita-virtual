{
  "version": 2,
  "functions": {
    "api/[...path].js": {
      "includeFiles": "backend/**"
    },
    "api/health.js": {}
  },
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/public/:path*",
      "destination": "/api/public/:path*"
    }
  ]
}

