{
  "version": 2,
  "builds": [
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/health",
      "dest": "/api/health.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/test",
      "dest": "/api/test.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(?!api|public|health|test)(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/*.js": {
      "includeFiles": "backend/**",
      "maxDuration": 30
    }
  }
}
